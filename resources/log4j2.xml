<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="SOABA" packages="">
  <Appenders>
    <File name="soaba.log" fileName="logs/soaba.log">
      <PatternLayout>
        <Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
      </PatternLayout>
    </File>
  </Appenders>
  <Loggers>
    <Root level="error">
      <AppenderRef ref="soaba.log"/>
    </Root>
  </Loggers>
</Configuration>